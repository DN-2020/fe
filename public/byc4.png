    	            {{/if}}
    	            {{#if return_time_new}}
    	            <p class="pickUpDate t_gray">반납 일자:<span>{{format_standard_date_duration_new return_time_new @root._lang}}</span></p>
    	            {{/if}}
    	        {{else if (isSim activity_template_id)}}
    	            {{#if pickup_time_new}}
    	            <p class="pickUpDate t_gray">수령 일자 / 출국 일자:<span>{{format_standard_date_duration_new pickup_time_new @root._lang}}</span></p>
    	            {{/if}}
                {{else if (isYsim activity_template_id)}}
                    {{#if pickup_time_new}}
                        <p class="pickUpDate t_gray">수령일자:<span>{{format_standard_date_duration_new pickup_time_new @root._lang}}</span></p>
                    {{/if}}
    	        {{/if}}
    	        {{#if package_specs}}
                    <p class="pack_name t_gray">{{concatHtmlWifi package_specs 'attr_name' ' · '}}{{#pre_other_info}} · {{getPreOtherContent type_hint content position}}{{/pre_other_info}}</p>
                {{/if}}
            {{else}}
                <p class="t_black t_nowrap">{{package_name}}</p>
                <!-- [fnb openticket]  -->
                {{#if (and (isFNB activity_template_id)  (eq ticket_type 0))}}
                <p class="t_gray t_nowrap">유효기간 내 상시 사용 가능</p>
                {{else if (isHotelVoucher activity_template_id)}}
                    <p class="hotel_card_validate_date t_nowrap">유효기간: <span>{{format_standard_date_new hotel_begin_time @root._lang 2}} - {{format_standard_date_new hotel_end_time @root._lang 2}}</span></p>
                    </p>
                {{else}}
                <p class="t_gray t_nowrap">
                    날짜: {{format_standard_date_new selected_time @root._lang 2}}
                </p>
                {{/if}}
    
                <!-- [fnb openticket]  -->
    	        <p class="t_gray t_nowrap">
                    수량:{{#price_items}}
    	            {{count}} {{name}}&nbsp;&nbsp;&nbsp;
    	            {{/price_items}}
    	        </p>
            {{/if}}
            <p class="t_main">{{@root._currency_symbo}} {{formatPriceThousands ticket_sell_price}}</p>
        </div>
    </a>
    </li>
    {{/items}}
    </ul>
    <div class="bottom_box m_bg_white">
        <div class="sum_box">
            <p class="t_black t14">{{t 'global.shoppingcart.sum' items.length}}</p>
            <p class="t_main t16 t_nowrap">{{_currency_symbo}} {{formatPriceThousands sum}}</p>
        </div>
        <div class="klk_spm_module_box klk_spm_module_box--auto" data-spm-module="ShoppingCartBox_GoTo">
            <button class="m_btn_main j_nav_shopcart t16" data-spm-item="__default?typ=entry" style="width: 100%">장바구니로 이동</button>
        </div>
    </div>
    
                            </script>
